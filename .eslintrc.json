{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.css"],
      "rules": {
        "css/no-duplicate-properties": "warn",
        "css/no-empty-rules": "warn"
      }
    }
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
